<h2>Batches in queue not yet complete</h2>
<table class='table table-striped'>
  <thead>
    <tr>
      <th class='col-xs-1'>Batch</th>
      <th class='col-xs-2'>Load date</th>
      <th class='col-xs-3'>Loaded by</th>
      <th class='col-xs-4'>Report to</th>
      <th class='col-xs-5'>Original library</th>
      <th class='col-xs-6'>Action</th>
      <th class='col-xs-7'>Number of items</th>
      <th class='col-xs-8'>Number of items left</th>
    </tr>
  </thead>
  <tbody>
    <% @batches_not_complete.each do |b| %>
      <tr class='<%= cycle("odd", "even") %>'>
        <td class='col-xs-1'><%= b['batch_id'] %></td>
        <td class='col-xs-2'><%= b['min(b.batch_date)'] %></td>
        <td class='col-xs-3'><%= b['min(b.user_name)'] %></td>
        <td class='col-xs-4'><%= b['min(b.user_email)'] %></td>
        <td class='col-xs-5'><%= b['min(b.orig_lib)'] %></td>
        <td class='col-xs-6'><%= b['min(b.action)'] %></td>
        <td class='col-xs-7'><%= b['min(b.total_bcs)'] %></td>
        <td class='col-xs-8'><%= b['count(u.pending)'] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<div class="btn-group">
  <%= main_menu_button %>
  <%= batch_menu_button %>
</div>
